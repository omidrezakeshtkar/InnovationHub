{
	"name": "innovation-hub-backend",
	"version": "2.0.0",
	"main": "dist/server.js",
	"engines": {
		"node": ">=20.0.0"
	},
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"start": "node dist/server.js",
		"predev": "docker compose -f ../docker-compose.dev.yml up -d",
		"dev": "nodemon -L src/server.ts",
		"postdev": "docker compose -f ../docker-compose.dev.yml down",
		"prebuild": "rimraf dist",
		"build": "tsc && copyfiles -u 1 src/**/*.{json,yaml,html,js} dist",
		"docker:build": "docker build -t InnovationHub-backend .",
		"docker:run": "docker run -p 3000:3000 InnovationHub-backend",
		"migrate:up": "migrate-mongo up -f src/migrations/migrate-mongo-config.js",
		"migrate:down": "migrate-mongo down -f src/migrations/migrate-mongo-config.js",
		"migrate:status": "migrate-mongo status -f src/migrations/migrate-mongo-config.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "^7.1.2",
		"@react-email/components": "0.0.25",
		"bcrypt": "^5.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.17.1",
		"express-rate-limit": "^6.3.0",
		"helmet": "^4.6.0",
		"joi": "^17.13.3",
		"jsonwebtoken": "^8.5.1",
		"migrate-mongo": "^9.0.0",
		"mongodb": "^4.1.3",
		"mongoose": "^6.10.0",
		"nodemailer": "^6.9.14",
		"rate-limit-redis": "^4.2.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"redis": "^4.7.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-dist": "^5.0.0",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.14.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jest": "^29.5.12",
		"@types/joi": "^17.2.3",
		"@types/jsonwebtoken": "^8.5.5",
		"@types/migrate-mongo": "^8.2.0",
		"@types/mongodb": "^4.0.7",
		"@types/node": "^16.11.6",
		"@types/nodemailer": "^6.4.15",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@types/supertest": "^6.0.2",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@types/uuid": "^10.0.0",
		"copyfiles": "^2.4.1",
		"jest": "^29.7.0",
		"nodemon": "^2.0.14",
		"rimraf": "^6.0.1",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.4.0",
		"typescript": "^4.4.4"
	}
}
